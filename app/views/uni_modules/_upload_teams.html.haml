.modal.fade#teamUploadModal{"tabindex":"-1", "role": "dialog", "aria-labelledby": "teamUploadModalLabel", "aria-hidden": "true"}
  .modal-dialog.modal-lg{"role": "document"}
    .modal-content
      .modal-header
        %h5.modal-title#teamUploadModalLabel= "Upload teams from CSV file"
        %button.close{"type": "button", "data-dismiss": "modal", "aria-label": "Close"}
          %span{"aria-hidden": "true"} &times;
      .modal-body
        Please attach a CSV file in the format
        %i username,team_no
        with each username on a different line.
        = form_tag({:action => :team_process}, multipart: true) do
          = hidden_field_tag 'mod_id', @mod_id
          = file_field_tag :file
          = submit_tag( "Import" )
      .modal-footer
        %button.btn.btn-secondary{"type": "button", "data-dismiss": "modal"}
          Close
