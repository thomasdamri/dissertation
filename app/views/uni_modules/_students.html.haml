.modal.fade#allStudentsModal{"tabindex":"-1", "role": "dialog", "aria-labelledby": "allStudentsModalLabel", "aria-hidden": "true"}
  .modal-dialog.modal-lg{"role": "document"}
    .modal-content
      .modal-header
        %h5.modal-title#allStudentsModalLabel= "All students for #{@uni_module.title}"
        %button.close{"type": "button", "data-dismiss": "modal", "aria-label": "Close"}
          %span{"aria-hidden": "true"} &times;
      .modal-body
        %table.table.table-striped.table-hover.table-bordered#allStudentTable
          %tr
            %th Team Number
            %th Student Name
            %th Student Username
            %th Student Email
          - @uni_module.teams.order(:number).each do |team|
            - team.users.each do |user|
              %tr
                %td= link_to "Team #{team.number}", team_path(team)
                %td= user.real_display_name
                %td= user.username
                %td= user.email
      .modal-footer
        %button.btn.btn-secondary{"type": "button", "data-dismiss": "modal"}
          Close
